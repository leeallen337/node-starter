{
  "name": "node-starter",
  "version": "0.0.1",
  "description": "A simple boilerplate node app",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --format 'node_modules/eslint-formatter-friendly' './**/*.js'",
    "start": "nodemon server.js",
    "start:debug": "nodemon --inspect-brk server.js",
    "start:prod": "node server.js",
    "test": "NODE_ENV=test mocha -R spec --file test/support/setup.js 'test/**/*.spec.js' --exit",
    "test:debug": "npm run test -- --inspect-brk --no-timeouts"
  },
  "author": "Lee Allen",
  "license": "MIT",
  "devDependencies": {
    "chai": "^4.2.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^5.16.0",
    "eslint-config-prettier": "^4.3.0",
    "eslint-formatter-friendly": "^6.0.0",
    "faker": "^4.1.0",
    "husky": "^2.3.0",
    "mocha": "^6.1.4",
    "nodemon": "^1.19.1",
    "prettier": "^2.0.2",
    "pretty-quick": "^1.11.0",
    "rosie": "^2.0.1",
    "sinon": "^7.3.2",
    "sinon-chai": "^3.3.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "helmet": "^3.18.0",
    "knex": "^0.17.0",
    "lodash": "^4.17.11",
    "objection": "^1.6.9",
    "objection-db-errors": "^1.1.1",
    "pg": "^7.11.0"
  }
}
