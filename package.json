{
  "name": "node-starter",
  "version": "0.0.1",
  "description": "A simple boilerplate node app",
  "main": "index.js",
  "scripts": {
    "lint": "eslint --format 'node_modules/eslint-formatter-friendly' './**/*.js'",
    "start": "nodemon server.js",
    "start:debug": "nodemon --inspect-brk server.js",
    "start:prod": "node server.js",
    "test": "NODE_ENV=test mocha 'src/**/*.spec.js' 'test/**/*.test.js'",
    "test:debug": "npm run test -- --inspect-brk --no-timeouts"
  },
  "author": "Lee Allen",
  "license": "MIT",
  "devDependencies": {
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "^4.1.0",
    "husky": "^4.2.3",
    "mocha": "^7.1.1",
    "nodemon": "^1.19.1",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "rosie": "^2.0.1",
    "sinon": "^9.0.1",
    "sinon-chai": "^3.5.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "knex": "^0.20.13",
    "lodash": "^4.17.15",
    "morgan": "^1.10.0",
    "objection": "^2.1.3",
    "pg": "^7.18.2"
  }
}
