{
  "name": "node-starter",
  "version": "0.0.1",
  "description": "A simple boilerplate node app",
  "main": "index.js",
  "scripts": {
    "build": "npm-run-all --serial clean transpile",
    "clean": "rimraf dist",
    "dev": "NODE_ENV=development DEBUG=api:*,knex:*,morgan:* npm-run-all --serial build server",
    "dev:debug": "NODE_ENV=development DEBUG=api:*,knex:*,morgan:* npm-run-all --serial build server:debug",
    "lint": "eslint --format 'node_modules/eslint-friendly-formatter' './**/*.js'",
    "prod": "NODE_ENV=production npm-run-all --serial build server",
    "serve": "npm run prod",
    "server": "node dist/server.js",
    "server:debug": "node --inspect-brk dist/server.js",
    "start": "nodemon --exec npm run dev",
    "start:debug": "nodemon --exect npm run dev:debug",
    "test": "NODE_ENV=test mocha 'test/**/*.spec.js'",
    "test:debug": "npm run test -- --inspect-brk",
    "transpile": "babel src/ -d dist/ --source-maps inline"
  },
  "author": "Lee Allen",
  "license": "MIT",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/register": "^7.9.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "dirty-chai": "^2.0.1",
    "eslint": "^6.8.0",
    "eslint-config-prettier": "^6.10.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-prettier": "^3.1.2",
    "faker": "^4.1.0",
    "husky": "^4.2.3",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.2",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.0.2",
    "pretty-quick": "^2.0.1",
    "rimraf": "^3.0.2",
    "rosie": "^2.0.1",
    "sinon": "^9.0.1",
    "sinon-chai": "^3.5.0",
    "supertest": "^4.0.2"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "debug": "^4.1.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^3.22.0",
    "knex": "^0.20.13",
    "lodash": "^4.17.15",
    "morgan": "^1.10.0",
    "objection": "^2.1.3",
    "pg": "^7.18.2"
  }
}
